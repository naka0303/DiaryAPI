<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.UsersCommandMapper">
    <insert id="createUser"
    		keyProperty="user_id" 
    		parameterType="com.example.demo.application.users.dto.InsertUsersDto">
        INSERT INTO users
        (
        	username,
        	age,
        	email,
        	password,
        	auth,
        	created_at
        )
        VALUES
        (
        	#{username},
        	#{age},
        	#{email},
        	#{password},
        	#{auth},
        	CURRENT_TIMESTAMP
        )
    </insert>
</mapper>